{
  "kind": "List",
  "apiVersion": "v1",
  "metadata": {
    "annotations": {
      "description": "Examples that can be installed into your project to allow you to test the EAP7 template. You should replace the contents with data that is more appropriate for your deployment."
    }
  },
  "labels": {
    "template": "sso-app-secret"
  },
  "items": [
    {
      "kind": "ServiceAccount",
      "apiVersion": "v1",
      "metadata": {
        "name": "sso-service-account"
      },
      "secrets": [
        {
          "name": "sso-app-secret"
        }
      ]
    },
    {
      "kind": "Secret",
      "apiVersion": "v1",
      "metadata": {
        "name": "sso-app-secret",
        "annotations": {
          "description": "Demo secrets file. jgroups.jceks contains a key named 'secret-key' with password 'password', and keystore.jks name 'jboss' and password 'mykeystorepass'"
        }
      },
      "data": {
        "jgroups.jceks": "",
        "keystore.jks": ""
      },
      "type": "Opaque"
    }
  ]
}