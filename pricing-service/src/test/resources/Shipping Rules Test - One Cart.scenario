<scenario>
  <name>Shipping Rules Test - One Cart.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>shipping-rules</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value>0.00</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>$sc</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace>
      <executionTimeResult>7</executionTimeResult>
      <numberOfRulesFired>1</numberOfRulesFired>
    </execution-trace>
    <expect-rule>
      <ruleName>Row 1 Shipping Rules</ruleName>
      <expectedCount>1</expectedCount>
      <explanation>Rule [Row 1 Shipping Rules] activated 1 times.</explanation>
      <successResult>true</successResult>
      <actualResult>1</actualResult>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>shippingTotal</fieldName>
          <expected>2.99</expected>
          <successResult>true</successResult>
          <explanation>[$sc] field [shippingTotal] was [2.99].</explanation>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$sc</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value>24.00</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>$sc2</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace>
      <executionTimeResult>1</executionTimeResult>
      <numberOfRulesFired>1</numberOfRulesFired>
    </execution-trace>
    <expect-rule>
      <ruleName>Row 1 Shipping Rules</ruleName>
      <expectedCount>1</expectedCount>
      <explanation>Rule [Row 1 Shipping Rules] activated 1 times.</explanation>
      <successResult>true</successResult>
      <actualResult>1</actualResult>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>shippingTotal</fieldName>
          <expected>2.99</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$sc2</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value>25.00</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>$sc3</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>Row 2 Shipping Rules</ruleName>
      <expectedCount>2</expectedCount>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>shippingTotal</fieldName>
          <expected>4.99</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$sc3</name>
      <anonymous>false</anonymous>
    </expect-fact>
  </fixtures>
  <lastRunResult>2017-01-26 12:50:28.659 UTC</lastRunResult>
  <rules/>
  <inclusive>false</inclusive>
  <packageName></packageName>
